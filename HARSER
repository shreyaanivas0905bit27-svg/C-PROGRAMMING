#include<iostream>
#include<cstring>
#include<string>
using namespace std;
class harser
{
public:
float a[10];
public:
void harmonic(int n,int a,int d,int*harr)
{
    int i;
for(i=0;i<n;i++)
{
harr[i]=(1/a+(i*d));
cout<<harr[i]<<endl;
}
}
};
int main()
{
int a,d;
int*harr,n;
harr=new int[20];
cout<<"n:";
cin>>n;
cout<<"a:";
cin>>a;
cout<<"d:";
cin>>d;
harser h;
h.harmonic(n,a,d,harr);
return 0;
}
